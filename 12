local Tab1 = Init:NewTab("Create Items")

	local Selector1 = Tab1:NewSelector("Food giver", "Click Here!", {"Apple", "Chips", "Cookie", "BloxyCola", "Pizza3", "Lollipop"}, function(d)
game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("GiveTool"):FireServer(d)
end)


local Label2 = Tab1:NewLabel("< Create Health Items >", "center")
	local Selector1 = Tab1:NewSelector("Health Items Giver", "Click Here!", {"Cure", "Medkit"}, function(f)
game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("GiveTool"):FireServer(f)
end)

local Section1 = Tab1:NewSection("Weapons")

local Label3 = Tab1:NewLabel("< Create Basic Weapons >", "center")
local Selector1 = Tab1:NewSelector("Basic Weapons Giver", "Click Here!", {"Broom", "Breaker", "Bat", "Hammer", "Crowbar", "Pitchfork"}, function(q)
    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("BasementWeapon"):FireServer(true,q)
end)    


    local Label3 = Tab1:NewLabel("< Create Advanced Weapons >", "center")
    local Selector1 = Tab1:NewSelector("Advanced Weapons Giver", "Click Here!", {"Gun", "SwatGun"}, function(q)
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("BasementWeapon"):FireServer(true,q)
end)    

